{
  "name": "simple-oauth2-server",
  "version": "2.0.0",
  "description": "Simple module for deploying oAuth2 server with several levels of protection. Perfect work with ember-simple-auth",
  "main": "index.js",
  "scripts": {
    "start": "cd example/ && node app.js",
    "build": "babel ./versions/Node-v7/soas2.js --minified -o ./versions/Node-v6/soas2.js -e 'process.env.build = `true`'",
    "watch": "babel ./versions/Node-v7/soas2.js -w --minified -o ./versions/Node-v6/soas2.js"
  },
  "keywords": [
    "oauth2",
    "bearer",
    "ember-simple-auth",
    "several layers",
    "protect routes"
  ],
  "author": "justerest",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "lowdb": "^0.16.2",
    "moment": "^2.18.1",
    "promise-any": "^0.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "mysql": "^2.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justerest/simple-oauth2-server.git"
  },
  "bugs": {
    "url": "https://github.com/justerest/simple-oauth2-server/issues"
  },
  "homepage": "https://github.com/justerest/simple-oauth2-server#readme",
  "directories": {
    "example": "example"
  }
}
